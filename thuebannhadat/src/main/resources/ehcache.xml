<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd"
	updateCheck="true" monitoring="autodetect" dynamicConfig="true">
	<diskStore path="java.io.tmpdir/ehcache" />
	<defaultCache maxElementsInMemory="100000"
		memoryStoreEvictionPolicy="LFU" eternal="false" timeToIdleSeconds="0"
		timeToLiveSeconds="0" overflowToDisk="false"
		diskExpiryThreadIntervalSeconds="0">
	</defaultCache>

	<cache name="org.hibernate.cache.StandardQueryCache"
		maxElementsInMemory="5000" memoryStoreEvictionPolicy="LFU" eternal="false"
		timeToLiveSeconds="0" overflowToDisk="false"
		diskExpiryThreadIntervalSeconds="0">
	</cache>

	<cache name="jp.gmo.zcom.ticket.entity.Stadium" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="jp.gmo.zcom.ticket.entity.TicketPrice" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="jp.gmo.zcom.ticket.entity.Stand" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="jp.gmo.zcom.ticket.entity.SeatType" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="jp.gmo.zcom.ticket.entity.Seat" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
		
	<cache name="jp.gmo.zcom.ticket.entity.Match" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

	<cache name="jp.gmo.zcom.ticket.entity.Line" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

	<cache name="jp.gmo.zcom.ticket.entity.Gate" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />

	<cache name="jp.gmo.zcom.ticket.entity.Floor" maxElementsInMemory="100000"
		eternal="false" timeToIdleSeconds="0" timeToLiveSeconds="0"
		overflowToDisk="false" memoryStoreEvictionPolicy="LRU" />
</ehcache>