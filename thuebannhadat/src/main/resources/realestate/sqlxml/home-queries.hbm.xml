<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<query name="HomeDao.getAllCity">
	<![CDATA[
        SELECT c
        FROM City c
        WHERE c.status = :status
        ]]>
	</query>

</hibernate-mapping>